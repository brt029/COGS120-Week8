          </a>
          <div align="right">
          {{summary}}</div>
        </div>
      </div>
    {{/each}}
          
          {{else}}

<div class="row">
  <div class="column" style="background-color:#aaa;" align="center">
    <h3><a class="linkClick" href="cardio">Intro to Cardio</a></h3>
  </div>

  <div class="column" style="background-color:#bbb;" align="center">
    <h3><a class="linkClick" href="basic">Basic Routines</a></h3>
  </div>
</div>

<div class="row">
  <div class="column" style="background-color:#ccc;" align="center">
    <h3><a class="linkClick" href="weightlift">Intro to Weightlifting</a></h3>
  </div>
  <div class="column" style="background-color:#ddd;" align="center">
    <h3><a class="linkClick" href="leg">Leg Routines</a></h3>
  </div>
  <div class="column" style="background-color:#eee;" align="center">
    <h3><a class="linkClick" href="killer">Killer Cardio Routines</a></h3>
  </div>
</div>
{{/if}}

<div class="push"></div>
</div>

