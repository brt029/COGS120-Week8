</div>

{{#if viewAlt}}

          {{#each routines}}
          <div class="row">
      <div class="column" style="background-color:#aaa;" id="{{id}}"> 
        <div class="post-title">
      <img src="{{image}}" alt="exercises" width="70" height="58"></div>
            <a class="linkCtr" href="/{{link}}">
